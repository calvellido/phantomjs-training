  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/calvellido/phantomjs-training/issues"
  },
  "homepage": "https://github.com/calvellido/phantomjs-training#readme",
  "dependencies": {
    "phantomjs": "^2.1.7",
    "webpack": "^1.13.3"
  },
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-preset-es2015": "^6.22.0",
    "babel-runtime": "^6.23.0",
    "rimraf": "~2.5.4"
  }
}
