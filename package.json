{
  "name": "phantomjs-training",
  "version": "0.0.1",
  "description": "This a simple playground to test PhantomJS features",
  "main": "github.js",
  "scripts": {
    "build": "webpack --config webpack.config.js --progress --profile",
    "clean": "npm cache clean && npm run rimraf -- node_modules build",
    "rimraf": "rimraf",
    "start": "npm run build && phantomjs build/main.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/calvellido/phantomjs-training.git"
  },
  "keywords": [
    "phantomjs"
  ],
